{
  "name": "{{ name }}",
{{#if_eq builder 'packager'}}
  "productName": "{{ name }}",
{{/if_eq}}
  "version": "0.0.0",
  "description": "{{ description }}",
  "main": "./dist/electron/main.js",
  "scripts": {
    {{#if_eq builder 'packager'}}
    "build": "node .electron-vue/build.js",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:darwin": "cross-env BUILD_TARGET=darwin node .electron-vue/build.js",
    "build:linux": "cross-env BUILD_TARGET=linux node .electron-vue/build.js",
    "build:mas": "cross-env BUILD_TARGET=mas node .electron-vue/build.js",
    "build:win32": "cross-env BUILD_TARGET=win32 node .electron-vue/build.js",
    {{else}}
    "build": "node .electron-vue/build.js && build",
    "build:dir": "node .electron-vue/build.js && build --dir",
    {{/if_eq}}
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    {{#if e2e}}
    "e2e": "npm run pack && mocha test/e2e",
    {{/if}}
    {{#if eslint}}
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src{{#testing unit e2e}} test{{/testing}}",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src{{#testing unit e2e}} test{{/testing}}",
    {{/if}}
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    {{#testing unit e2e}}
    "test": "{{#unit}}npm run unit{{/unit}}{{#unit}}{{#e2e}} && {{/e2e}}{{/unit}}{{#e2e}}npm run e2e{{/e2e}}",
    {{/testing}}
    {{#if unit}}
    "unit": "karma start test/unit/karma.conf.js",
    {{/if}}
    "postinstall": "{{#if eslint}}npm run lint:fix{{/if}}"
  },
  {{#if_eq builder 'builder'}}
  "build": {
    "productName": "{{ name }}",
    "appId": "org.simulatedgreg.electron-vue",
    "category": "public.app-category.tools",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron",
      "node_modules/",
      "package.json"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
{{/if_eq}}
  "author": "{{ author }}",
  "license": null,
  "dependencies": {
    "vue": "^2.2.2"{{deps plugins}}
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.3.3",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.2.0",
    "babili-webpack-plugin": "^0.0.11",
    "cfonts": "^1.1.3",
    "chalk": "^1.1.3",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.1.4",
    "css-loader": "^0.27.3",
    "del": "^2.2.1",
    "devtron": "^1.1.0",
    "electron": "^1.3.1",
    "electron-debug": "^1.1.0",
    "electron-devtools-installer": "^2.0.1",
    {{#if_eq builder 'packager'}}
    "electron-packager": "^8.5.0",
    {{else}}
    "electron-builder": "^11.4.4",
    {{/if_eq}}
    "electron-rebuild": "^1.1.3",
    {{#eslint}}
    "babel-eslint": "^7.0.0",
    "eslint": "^3.13.1",
    "eslint-friendly-formatter": "^2.0.5",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-html": "^2.0.0",
    {{#if_eq eslintConfig 'standard'}}
    "eslint-config-standard": "^7.1.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    {{/if_eq}}
    {{#if_eq eslintConfig 'airbnb'}}
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.8.1",
    {{/if_eq}}
    {{/eslint}}
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.16.1",
    "json-loader": "^0.5.4",
    {{#if unit}}
    "babel-plugin-istanbul": "^4.1.1",
    "inject-loader": "^3.0.0",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-electron": "^5.1.1",
    "karma-mocha": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.30",
    "karma-webpack": "^2.0.1",
    "webpack-merge": "^4.1.0",
    {{/if}}
    {{#if e2e}}
    "require-dir": "^0.3.0",
    "spectron": "^3.4.0",
    {{/if}}
    {{#testing unit e2e}}
    "chai": "^3.5.0",
    "mocha": "^3.0.2",
    {{/testing}}
    "multispinner": "^0.2.1",
    "style-loader": "^0.16.0",
    "tree-kill": "^1.1.0",
    "url-loader": "^0.5.7",
    "vue-html-loader": "^1.2.2",
    "vue-loader": "^11.3.3",
    "vue-style-loader": "^2.0.4",
    "vue-template-compiler": "^2.2.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0",
    "webpack-hot-middleware": "^2.18.0"
  }
}
